#include <stdio.h>
#include <stdlib.h>

void selection_sort(int array[],int n){
	int temp;
	for(int i =0;i<n;i++){
		if(array[i] >array[i+1]){
			temp = array[i];
			array[i] = array[i+1];
			array[i+1] = temp;
		}
	}
}

int main(void) {
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
