#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    FILE* fp;
    char  line[255];

    fp = fopen("word.txt" , "r");
    while (fgets(line, sizeof(line), fp) != NULL)
    {
        const char* val1 = strcpy(val1,  strtok(line,"\0"));

        printf("%s\n", val1);
    }
}
